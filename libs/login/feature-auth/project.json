{
  "projectType": "library",
  "sourceRoot": "libs/login/feature-auth/src",
  "prefix": "login",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": [
        "dist/libs/login/feature-auth"
      ],
      "options": {
        "project": "libs/login/feature-auth/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/login/feature-auth/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/login/feature-auth/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/login/feature-auth"
      ],
      "options": {
        "jestConfig": "libs/login/feature-auth/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/login/feature-auth/**/*.ts",
          "libs/login/feature-auth/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "domain:login",
    "type:feature"
  ]
}
